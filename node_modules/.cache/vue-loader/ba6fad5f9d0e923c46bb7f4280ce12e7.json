iv>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nconst itemGetter = require('./../mixins/mixin').itemGetter;\r\n\r\nexport default {\r\n  name: \"Replies\",\r\n  mixins: [itemGetter],\r\n\r\n  props: {\r\n    replyIds: Array\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      replies: []\r\n    };\r\n  },\r\n  \r\n  methods: {\r\n    getReplies() {\r\n      const promises = [];\r\n      if (this.replyIds) {\r\n        for (const itemId of this.replyIds) {\r\n          promises.push(this.getItemById(itemId));\r\n        }\r\n      }\r\n\r\n      Promise.all(promises)\r\n        .then(res => {\r\n          res.forEach(item => {\r\n            this.replies.push(item);\r\n          });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getReplies();\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n\r\n.margin-for-reply {\r\n  margin-left: 20px;\r\n}\r\n\r\n</style>"]}]}