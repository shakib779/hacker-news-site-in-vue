n\r\n  props: {\r\n    reply: Object\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      replies: []\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    indent() {\r\n      return { transform: `translate(${20}px)` };\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    getReplies() {\r\n      const promises = [];\r\n      if (this.reply.kids) {\r\n        for (const itemId of this.reply.kids) {\r\n          promises.push(\r\n            this.$http.get(\r\n              url.getItemById +\r\n                itemId.toString() +\r\n                \".json\"\r\n            )\r\n          );\r\n        }\r\n      }\r\n\r\n      Promise.all(promises)\r\n        .then(res => {\r\n          res.forEach(item => {\r\n            this.replies.push(item.data);\r\n          });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getReplies();\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n</style>"]}]}