>{{post.descendants}} points | by {{post.by}} {{post.time | getTimeDiffrence }} ago</h5>\n\n    <!-- Comment section  -->\n    <h4 v-if=\"post.kids\">{{post.kids.length}} comments</h4>\n    <div v-if=\"post.kids\">\n      <Replies v-bind:replyIds=\"post.kids\"/>\n    </div>\n    <br />\n  </div>\n</template>\n\n<script>\nimport Replies from \"./Replies\";\nconst itemGetter = require('./../mixins/mixin').itemGetter;\n\nexport default {\n  name: \"Comments\",\n  components: { Replies },\n  mixins: [itemGetter],\n  data() {\n    return {\n      post: {}\n    };\n  },\n\n  methods: {\n    \n    /**\n     * Params: post id from url\n     * Fetch the post info by the id\n     */\n    getPostInfo(id) {\n      this.getItemById(id)\n        .then(res => {\n          this.post = res;\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    }\n  },\n\n  /**\n   * Initial function of the component \n   */\n  created() {\n    if (this.$route.params.postId) {\n      this.getPostInfo(this.$route.params.postId);\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n</style>\n"],"sourceRoot":"src/components"}]}