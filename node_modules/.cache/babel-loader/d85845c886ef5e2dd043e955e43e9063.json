O,EAAE;AACL;;;AAGAC,IAAAA,WAAW,EAAE,qBAACC,OAAD,EAAUC,GAAV,EAAkB;AAC3B,aAAO,IAAIC,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACpCtB,QAAAA,GAAG,CAACE,KAAJ,CAAUqB,GAAV,CAAcJ,GAAd,EACKK,IADL,CACU,UAACC,GAAD,EAAS;AACXP,UAAAA,OAAO,CAACQ,MAAR,CAAe,YAAf,EAA6BD,GAAG,CAACE,IAAjC;AACAN,UAAAA,OAAO,CAACI,GAAG,CAACE,IAAL,CAAP;AACH,SAJL,EAKKC,KALL,CAKW,UAAAC,GAAG,EAAI;AACVP,UAAAA,MAAM,CAACO,GAAD,CAAN;AACH,SAPL;AAQH,OATM,CAAP;AAUH,KAfI;;AAiBL;;;AAGAC,IAAAA,gBAAgB,EAAE,0BAACZ,OAAD,EAAUa,MAAV,EAAqB;AACnC,aAAO,IAAIX,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACpC,YAAMU,QAAQ,GAAG,EAAjB;;AACA,aAAK,IAAIC,CAAC,GAAGF,MAAM,CAACG,UAApB,EAAgCD,CAAC,GAAGF,MAAM,CAACI,SAA3C,EAAsDF,CAAC,EAAvD,EAA2D;AACvDD,UAAAA,QAAQ,CAACI,IAAT,CAAc/B,UAAU,CAACgC,OAAX,CAAmBC,WAAnB,CAA+BpB,OAAO,CAACT,KAAR,CAAcC,OAAd,CAAsBuB,CAAtB,EAAyBM,QAAzB,EAA/B,CAAd;AACH;;AAEDnB,QAAAA,OAAO,CAACoB,GAAR,CAAYR,QAAZ,EACKR,IADL,CACU,UAACiB,QAAD,EAAc;AAChBpB,UAAAA,OAAO,CAACoB,QAAD,CAAP;AACH,SAHL,EAIKb,KAJL,CAIW,UAAAC,GAAG,EAAI;AACVP,UAAAA,MAAM,CAACO,GAAD,CAAN;AACH,SANL;AAOH,OAbM,CAAP;AAcH;AAnCI;AArBuB,CAAf,CAAd","sourcesContent":["import Vue from 'vue';\nimport Vuex from 'vuex';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios'\n\n\nVue.use(Vuex);\nVue.use(VueAxios, axios);\n\nconst itemGetter = require('./../mixins/mixin').itemGetter;\n\nexport const store = new Vuex.Store({\n    state: {\n        itemIds: []\n    },\n\n    getters: {\n        itemIds: (state) => {\n            return state.itemIds;\n        },\n\n        itemIdLength: (state) => {\n            return state.itemIds.length;\n        }\n    },\n\n    mutations: {\n        setItemIds: (state, itemIds) => {\n            state.itemIds = itemIds;\n        }\n    },\n\n    actions: {\n        /**\n         * get post id list\n         */\n        getPostList: (context, url) => {\n            return new Promise((resolve, reject) => {\n                Vue.axios.get(url)\n                    .then((res) => {\n                        context.commit('setItemIds', res.data);\n                        resolve(res.data);\n                    })\n                    .catch(err => {\n                        reject(err);\n                    });\n            });\n        },\n\n        /**\n         * get post info for a particular range\n         */\n        getPostListRange: (context, object) => {\n            return new Promise((resolve, reject) => {\n                const promises = [];\n                for (let i = object.startIndex; i < object.lastIndex; i++) {\n                    promises.push(itemGetter.methods.getItemById(context.state.itemIds[i].toString()));\n                }\n\n                Promise.all(promises)\n                    .then((response) => {\n                        resolve(response);\n                    })\n                    .catch(err => {\n                        reject(err);\n                    })\n            });\n        }\n    }\n})"]}]}